  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "pack:native": "tsc -p tsconfig.native.json",
    "serve:native": "electron dist/native",
    "build:native": "electron-builder build",
    "start:native": "wait-on http://localhost:8888 && npm run pack:native && npm run serve:native",
