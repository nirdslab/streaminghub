    "@angular/router": "~11.0.1",
    "ace-builds": "^1.4.12",
    "ace-code-editor": "^1.2.3",
    "rxjs": "~6.6.3",
    "tslib": "^2.0.3",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1100.2",
