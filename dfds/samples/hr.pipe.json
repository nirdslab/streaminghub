{
  "$schema": "https://www.cs.odu.edu/~cs_pjaya001/dfds/pipe.jsonschema",
  "inputs": {
    "bvp": {
      "@ref": "bvp.stream.json#"
    },
    "ibi": {
      "@ref": "ibi.stream.json#"
    }
  },
  "outputs": {
    "hr": {
      "name": "heart rate",
      "description": "heart rate calculated using IBI and BVP sensory data",
      "unit": "bpm",
      "frequency": 0,
      "index": {
        "t": {
          "name": "timestamp",
          "description": "Recording timestamp",
          "dtype": "f32"
        }
      },
      "fields": {
        "hr": {
          "name": "heart rate",
          "description": "heart rate calculated from IBI and BVP data",
          "dtype": "f32"
        }
      }
    }
  },
  "uri": "./hr_pipe.py"
}