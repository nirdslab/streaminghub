{
  "$schema": "https://www.cs.odu.edu/~cs_pjaya001/dfds/node.jsonschema",
  "uri": "./hr_node.py",
  "consumes": {
    "bvp": {
      "@ref": "./empatica_e4.node.json#/generates/bvp"
    },
    "ibi": {
      "@ref": "./empatica_e4.node.json#/generates/ibi"
    }
  },
  "generates": {
    "hr": {
      "name": "heart rate",
      "description": "heart rate calculated using IBI and BVP sensory data",
      "unit": "bpm",
      "frequency": 0,
      "index": {
        "t": {
          "name": "timestamp",
          "description": "Recording timestamp",
          "dtype": "f32"
        }
      },
      "fields": {
        "hr": {
          "name": "heart rate",
          "description": "heart rate calculated from IBI and BVP data",
          "dtype": "f32"
        }
      }
    }
  }
}