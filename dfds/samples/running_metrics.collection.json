  "name": "Running_BVP_IBI_GSR",
  "description": "Blood volume pulse (bvp), inter-beat interval (ibi), and heart-rate (hr) measurements of 2 participants taken from Empatica E4 during a running task",
  "keywords": [
    "heart-rate",
    "running"
  ],
  "authors": [
    {
      "name": "John Doe",
      "affiliation": "Example University",
      "email": "john@example.edu"
    },
    {
      "name": "Jane Doe",
      "affiliation": "Example University",
      "email": "jane@example.edu"
    }
  ],
  "streams": {
    "bvp": {
      "@ref": "./bvp.stream.json#"
    },
    "ibi": {
      "@ref": "./ibi.stream.json#"
    },
    "hr": {
      "@ref": "./hr.pipe.json#"
    }
  },
  "groups": {
    "subject": {
      "description": "ID of the subject",
      "values": [
        "01",
        "02"
