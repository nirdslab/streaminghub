{
  "$schema": "https://json-schema.org/draft-07/schema",
  "description": "schema of an analytic",
  "type": "object",
  "required": [
    "$schema",
    "info",
    "sources",
    "fields",
    "inputs",
    "streams"
  ],
  "additionalProperties": false,
  "properties": {
    "$schema": {
      "type": "string"
    },
    "info": {
      "$ref": "https://raw.githubusercontent.com/nirdslab/streaminghub/master/dfs/dfs.schema.json#/definitions/info"
    },
    "sources": {
      "description": "data sources for generating the analytic",
      "$ref": "https://raw.githubusercontent.com/nirdslab/streaminghub/master/dfs/dfs.schema.json#/definitions/refs"
    },
    "fields": {
      "$ref": "https://raw.githubusercontent.com/nirdslab/streaminghub/master/dfs/dfs.schema.json#/definitions/fields"
    },
    "inputs": {
      "description": "information on the inputs consumed",
      "$ref": "https://raw.githubusercontent.com/nirdslab/streaminghub/master/dfs/dfs.schema.json#/definitions/refs"
    },
    "streams": {
      "description": "information on the output streams generated",
      "$ref": "https://raw.githubusercontent.com/nirdslab/streaminghub/master/dfs/dfs.schema.json#/definitions/streams"
    }
  }
}